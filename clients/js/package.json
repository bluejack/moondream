{
  "name": "moondream",
  "version": "0.5.0",
  "description": "Client for Moondream",
  "keywords": [
    "Moondream",
    "Client",
    "AI",
    "VLM"
  ],
  "author": "m87 labs",
  "license": "M87",
  "homepage": "https://moondream.ai",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "form-data": "^4.0.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "eslint": "^8.57.1",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": false,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "endOfLine": "auto",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "useTabs": false
        }
      },
      {
        "files": ".eslintrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  }
}
